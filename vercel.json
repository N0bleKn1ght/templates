{
    "rewrites": [
      { "source": "/manage", "destination": "/manage.html" }
    ],
    "trailingSlash": false,
    "headers": [
      {
        "source": "/public/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }